<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="clinicaBundle\Entity\Empleado" table="empleado">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="role" type="string" column="role" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellido" type="string" column="apellido" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dni" type="string" column="dni" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="integer" column="telefono" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechacontrato" type="date" column="fechacontrato" nullable="true"/>
    <field name="salariomes" type="integer" column="salariomes" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="imagen" type="string" column="imagen" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
